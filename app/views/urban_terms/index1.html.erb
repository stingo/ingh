<p id="notice"><%= notice %></p>

<h1>Urban Terms</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Inaction</th>
      <th>Postcover</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urban_terms.each do |urban_term| %>
      <tr>
        <td><%= urban_term.title %></td>
        <td><%= urban_term.description %></td>
        <td><%= urban_term.inaction %></td>
        <td><%= urban_term.postcover %></td>
        <td><%= link_to 'Show', urban_term %></td>
        <td><%= link_to 'Edit', edit_urban_term_path(urban_term) %></td>
        <td><%= link_to 'Destroy', urban_term, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Urban Term', new_urban_term_path %>

</br>

